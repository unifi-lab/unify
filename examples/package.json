{
  "name": "unify-server-examples",
  "version": "1.0.0",
  "description": "Examples for unify-server",
  "private": true,
  "type": "module",
  "scripts": {
    "basic-usage": "bun run --watch basic-usage.ts",
    "builtin-routes": "bun run --watch builtin-routes.ts",
    "validate-config": "bun run ../packages/unify-server/dist/cli.js validate-config blog-config.ts",
    "init-tables": "bun run ../packages/unify-server/dist/cli.js init-tables blog-config.ts",
    "generate-types": "bun run ../packages/unify-server/dist/cli.js generate-types blog-config.ts",
    "generate-methods": "bun run ../packages/unify-server/dist/cli.js generate-methods blog-config.ts",
    "setup": "bun run ../packages/unify-server/dist/cli.js setup blog-config.ts",
    "blog-server": "bun run --watch blog-server.ts"
  },
  "dependencies": {
    "unify-server": "workspace:*",
    "hono": "^4.7.11"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  }
}
