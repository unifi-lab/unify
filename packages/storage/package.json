{
  "name": "@unify/storage",
  "version": "0.0.1",
  "description": "Storage adapters for Unify framework",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
