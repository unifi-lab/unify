  "keywords": [
    "storage",
    "database",
    "adapter",
    "unify"
  ],
  "author": "osdodo",
  "license": "MIT",
  "dependencies": {
    "@unify/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "typescript": "^5.3.0"
  },
  "optionalDependencies": {
    "pg": "^8.11.3"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iotexproject/unify.git",
    "directory": "packages/storage"
