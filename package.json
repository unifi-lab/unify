{
  "name": "@unilab-monorepo",
  "version": "1.0.0",

  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "build:core": "cd packages/core && bun run build",
    "dev:core": "cd packages/core && bun run dev",
    "build:server": "cd packages/server && bun run build",
    "dev:server": "cd packages/server && bun run dev",
    "build:httply": "cd packages/httply && bun run build",
    "dev:httply": "cd packages/httply && bun run dev",
    "build:client": "cd packages/client && bun run build",
    "dev:client": "cd packages/client && bun run dev",
    "build:uniweb3": "cd packages/uniweb3 && bun run build",
    "dev:uniweb3": "cd packages/uniweb3 && bun run dev"
  },
  "author": "osdodo",
  "license": "MIT",
  "dependencies": {
    "@typescript-eslint/typescript-estree": "^8.35.0"
  }
}
