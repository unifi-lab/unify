{
  "name": "@unilab-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*",
    "docs"
  ],
  "scripts": {
    "build:urpc-core": "cd packages/urpc-core && bun run build",
    "dev:urpc-core": "cd packages/urpc-core && bun run dev",
    "build:urpc-hono": "cd packages/urpc-hono && bun run build",
    "dev:urpc-hono": "cd packages/urpc-hono && bun run dev",
    "build:urpc-next": "cd packages/urpc-next && bun run build",
    "dev:urpc-next": "cd packages/urpc-next && bun run dev",
    "build:urpc-client": "cd packages/urpc-client && bun run build",
    "dev:urpc-client": "cd packages/urpc-client && bun run dev",
    "build:urpc": "cd packages/urpc && bun run build",
    "dev:urpc": "cd packages/urpc && bun run dev",
    "build:uniweb3": "cd packages/uniweb3 && bun run build",
    "dev:uniweb3": "cd packages/uniweb3 && bun run dev",
    "build:urpc-ui": "cd packages/urpc-ui && bun run build",
    "build:uni-store": "cd packages/uni-store && bun run build",
    "dev:uni-store": "cd packages/uni-store && bun run dev",
    "build": "bun run build:urpc-core && bun run build:urpc-hono && bun run build:urpc-next && bun run build:urpc-client && bun run build:urpc && bun run build:uniweb3 && bun run build:urpc-ui && bun run build:uni-store"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@typescript-eslint/typescript-estree": "^8.35.0"
  }
}
