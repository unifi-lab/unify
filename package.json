{
  "name": "@unilab-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*",
    "docs"
  ],
  "scripts": {
    "build:core": "cd packages/core && bun run build",
    "dev:core": "cd packages/core && bun run dev",
    "build:unify-hono": "cd packages/unify-hono && bun run build",
    "dev:unify-hono": "cd packages/unify-hono && bun run dev",
    "build:unify-next": "cd packages/unify-next && bun run build",
    "dev:unify-next": "cd packages/unify-next && bun run dev",
    "build:unify-client": "cd packages/unify-client && bun run build",
    "dev:unify-client": "cd packages/unify-client && bun run dev",
    "build:unify": "cd packages/unify && bun run build",
    "dev:unify": "cd packages/unify && bun run dev",
    "build:uniweb3": "cd packages/uniweb3 && bun run build",
    "dev:uniweb3": "cd packages/uniweb3 && bun run dev",
    "build:store": "cd packages/store && bun run build",
    "dev:store": "cd packages/store && bun run dev",
    "build:unify-ui": "cd packages/unify-ui && bun run build",
    "build": "bun run build:core && bun run build:store && bun run build:unify-hono && bun run build:unify-next &&  bun run build:unify-client && bun run build:unify && bun run build:uniweb3 && bun run build:unify-ui"
  },
  "author": "osdodo",
  "license": "MIT",
  "dependencies": {
    "@typescript-eslint/typescript-estree": "^8.35.0"
  }
}
